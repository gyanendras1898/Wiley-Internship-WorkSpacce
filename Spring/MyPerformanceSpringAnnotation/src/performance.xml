<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:c="http://www.springframework.org/schema/c"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="singer" class="com.sujata.producer.Singer"
		c:song="Jinga Lala">
	</bean>
	<bean id="dancer" class="com.sujata.producer.Dancer"
		c:style="tak thak">
	</bean>
	<bean id="juggler" class="com.sujata.producer.Juggler"
		p:balls="23">
	</bean>
	<bean id="tabla" class="com.sujata.producer.Tabla" autowire-candidate="false"></bean>
	<bean id="guitar" class="com.sujata.producer.Guitar"></bean>

	<bean id="intrumentalist"
		class="com.sujata.producer.Instrumentalist" autowire="byType">
	</bean>
</beans>
