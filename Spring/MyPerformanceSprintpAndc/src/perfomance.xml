<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:c="http://www.springframework.org/schema/c"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="Shalini" class="com.sujata.producer.Singer" c:song="Gadi Wala Aya">
	</bean>
	
	<bean id="Waqar" class="com.sujata.producer.Juggler" p:balls="20">	
	</bean>
	
	<bean id="Chetan" class="com.sujata.producer.Dancer" c:style="Tandav">
	</bean>
	
	<bean id="guitar" class="com.sujata.producer.Guitar"></bean>
	<bean id="Tabla" class="com.sujata.producer.Tabla"></bean>
	<bean id="Drum" class="com.sujata.producer.Drum"></bean>
	
	<bean id="Shafi" class="com.sujata.producer.Instrumentalist" p:instrument-ref="guitar">
	</bean>

</beans>
