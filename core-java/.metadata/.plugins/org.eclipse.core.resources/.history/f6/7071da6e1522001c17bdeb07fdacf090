package tollplaza;

public class Main {
	public static void main(String[] args) {
		Tollplaza tollplaza=new Tollplaza();
		Vehicle v1=new Vehicle("Car",tollplaza);
		Vehicle v2=new Vehicle("Truck",tollplaza);
		Vehicle v3=new Vehicle("Bus",tollplaza);
		Vehicle v4=new Vehicle("auto",tollplaza);
		
		Thread thread1=new Thread(v1);
		Thread thread2=new Thread(v2);
		Thread thread3=new Thread(v3);
		Thread thread4=new Thread(v4);
		
		thread1.start();
		thread2.start();
		thread3.start();
		thread4.start();
	}
}
