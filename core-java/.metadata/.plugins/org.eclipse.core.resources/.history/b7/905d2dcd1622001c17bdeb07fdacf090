package tollplaza;

public class Main {
	public static void main(String[] args) {
		Tollplaza tollplaza=new Tollplaza();
		Vehicle vehicle1=new Vehicle("Car",tollplaza);
		Vehicle vehicle2=new Vehicle("Truck",tollplaza);
		Vehicle vehicle3=new Vehicle("Bus",tollplaza);
		Vehicle vehicle4=new Vehicle("auto",tollplaza);
		Vehicle vehicle5=new Vehicle("Airplane",tollplaza);
		
		Thread thread1=new Thread(vehicle1);
		Thread thread2=new Thread(vehicle2);
		Thread thread3=new Thread(vehicle3);
		Thread thread4=new Thread(vehicle4);
		Thread thread5=new Thread(vehicle5);
		
		thread1.start();
		thread2.start();
		thread3.start();
		thread4.start();
		thread5.start();
	}
}
